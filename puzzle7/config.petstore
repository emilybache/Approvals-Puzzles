# Full path to the System Under Test (or Java Main Class name)
executable:${TEXTTEST_HOME}/puzzle7/petstore.py
interpreter:python3

# Naming scheme to use for files for stdin,stdout and stderr
filename_convention_scheme:standard

# Expanded name to use for application
full_name:Petstore

[view_program]
config:subl
stdin:subl
stdout:subl
options:subl

[collate_file]
stdout:swagger_json
swagger_json:stdout

[collate_script]
swagger_json:python3 -m json.tool
